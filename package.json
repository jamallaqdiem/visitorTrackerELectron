{
  "name": "visitor-tracker-electron",
  "version": "1.0.0",
  "description": "Electron desktop wrapper for the Visitor Tracking and Management System.",
  "main": "main.js",
  "scripts": {
    "start": "npm run electron-dev",
    "electron-dev": "electron .",
    "build:client": "cd client && npm run build",
