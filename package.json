  "keywords": [
    "electron",
    "react",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.2.3",
    "electron-builder": "^24.0.0",
    "electron-rebuild": "^3.0.0"
  }
}
